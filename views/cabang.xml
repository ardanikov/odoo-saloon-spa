<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- cdn.cabang list view -->
        <record id="cdn_cabang_view_list" model="ir.ui.view">
            <field name="name">cdn.cabang.view.list</field>
            <field name="model">cdn.cabang</field>
            <field name="arch" type="xml">
                <list>
                    <field name="cabang_id"/>
                </list>
            </field>
        </record>

        <!-- cdn.cabang form view -->
        <record id="cdn_cabang_view_form" model="ir.ui.view">
            <field name="name">cdn.cabang.view.form</field>
            <field name="model">cdn.cabang</field>
            <field name="arch" type="xml">
                <form string="cabang">
                    <sheet>
                        <group>
                            <field name="cabang_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- cdn.cabang action window -->
        <record id="cdn_cabang_action" model="ir.actions.act_window">
            <field name="name">Data Cabang</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cdn.cabang</field>
            <field name="view_mode">list,form</field>
        </record>
        <menuitem id="cdn_cabang_menu" name="Cabang" parent="cdn_salon_menu_category" sequence="15" action="cdn_cabang_action"/>
    </data>
</odoo>